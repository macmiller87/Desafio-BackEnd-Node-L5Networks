// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  user_id   String   @id @default(uuid())
  name      String
  email     String
  password  String
  login     String   @unique
  createdAt DateTime @default(now())
}
